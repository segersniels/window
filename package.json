{
  "name": "@segersniels/window",
  "version": "1.0.9",
  "description": "Syntax highlighting in a nicely framed OSX like terminal window for usage with react-markdown",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "compile": "webpack",
    "compile:watch": "webpack --watch --progress",
    "check:typescript": "tsc --noEmit --skipLibCheck --project .",
    "lint": " eslint --ext ts,tsx .",
    "lint:fix": "eslint --ext ts,tsx . --fix",
    "clean": "rm -rf lib/*.js ; rm -rf lib/*.d.ts ; rm -rf lib/**/*.js ; rm -rf lib/**/*.d.ts"
  },
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react-syntax-highlighter": "^13.5.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.48",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "css-loader": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "prettier": "^2.0.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "^3.8.3",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/segersniels/window.git"
  },
  "keywords": [
    "react",
    "markdown",
    "osx",
    "terminal",
    "window",
    "ui",
    "code",
    "syntax",
    "highlighting"
  ],
  "author": "Niels Segers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/segersniels/window/issues"
  },
  "homepage": "https://github.com/segersniels/window#readme"
}
