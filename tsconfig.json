{
  "compilerOptions": {
    "declaration": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom", "es2016", "es2017"],
    "allowJs": false,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "jsx": "react"
  },
  "include": ["lib"]
}
